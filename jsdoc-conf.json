{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "plugins": ["node_modules/jsdoc-babel"],
    "babel": {
        "presets": [ "env" ],
        "plugins": [ 
            "transform-object-rest-spread",
            "transform-class-properties" ]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "destination": "./"          // same as -d ./out/
    },
    "include": ["src/*.js", "src/**.*.js"]
}